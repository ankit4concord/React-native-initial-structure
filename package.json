{
  "name": "intialsetupreactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "android:dev": "react-native run-android --variant=devdebug",
    "android:dev-release": "react-native run-android --variant=devrelease",
    "android:qa": "react-native run-android --variant=qadebug",
    "android:qa-release": "react-native run-android --variant=qarelease",
    "android:uat": "react-native run-android --variant=uatdebug",
    "android:uat-release": "react-native run-android --variant=uatrelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.0.7",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-config": "^1.5.0",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.18.2",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "lint-staged": "^13.2.0",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
